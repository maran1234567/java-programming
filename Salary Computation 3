import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
     Scanner sc = new Scanner(System.in);
        
        double salary = sc.nextInt();
        double hra,da;
        
        if(salary<15000){
            hra=(salary*0.15);
            da=(salary*0.16);
            
            System.out.println
    }
}
